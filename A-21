linha=0
coluna=0

mat=[[0]*5,[0]*5,[0]*5,[0]*5,[0]*5]

for linha in range(0,5,1):
    for coluna in range(0,5,1):
        mat[linha][coluna]:int(input(f"Insira um nÃºmero: "))

for linha in range(0,5,1):
    for coluna in range(0,5,1):
        if(linha==coluna):
            print(f"[{mat[linha][coluna]}]", end='')
